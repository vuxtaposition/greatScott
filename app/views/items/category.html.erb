      <section id="content">
        <div class="wrapper">
      
        <div id="container">
            <div id="myYield">

<h1>Listing items (Category: <%= @category %>)</h1>
<div id="tableSurround">
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Image url</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td class="porductName" ><%= item.name %></td>
        <td><%= item.description %></td>
        <td><%= item.cost %></td>
        <td><%= item.image %></td>
        <td><%= item.category %></td>
        <td ><span class="mybtn"><%= link_to 'Show', item %></span></td>
        <td><span class="mybtn"><%= link_to 'Edit', edit_item_path(item) %></span></td>
        <td ><span class="mybtn"><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></span></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Item', new_item_path %>
</div>
</div>
</div>
</section>